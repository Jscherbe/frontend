{
  "name": "@ulu/frontend",
  "version": "0.0.2",
  "description": "Frontend themeing library. No opinion, modular Sass library that can be used as a framework base, only works in Sass > 1.23. This is a slight refactor of the orginal @ulu/sass that will now include all frontend related assets (SCSS, JS, etc)",
  "browser": "js/index.js",
  "scripts": {
    "docs:dev": "vuepress dev docs --no-clear-screen",
    "docs:dev:debug": "vuepress dev docs --no-clear-screen --debug",
    "docs:build": "vuepress build docs",
    "serve": "webpack serve --mode development --progress --stats minimal --env docs",
    "build": "webpack build --mode production --progress --stats minimal --env docs",
    "sassdoc-test": "node --inspect-brk ./sassdoc-test.js",
    "sassdoc-test2": "node  ./sassdoc-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jscherbe/frontend.git"
  },
  "keywords": [
    "SCSS",
    "javascript",
    "framework",
    "mixin"
  ],
  "authors": {
    "name": "Joe Scherben",
    "email": "jscherbe@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jscherbe/frontend/issues"
  },
  "homepage": "https://github.com/Jscherbe/frontend#readme",
  "devDependencies": {
    "@ulu/vuepress-plugin-auto-nav": "^1.0.0",
    "@ulu/vuepress-plugin-sassdoc": "^1.0.1",
    "documentation": "^13.2.5",
    "documentation-theme-light": "^1.1.1",
    "sass": "^1.51.0",
    "vuepress": "^1.5.3"
  }
}
